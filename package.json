{
  "name": "CodeAnalyzer",
  "version": "0.1.0",
  "description": "A minimal Electron application",
  "main": "out/main.js",
  "private": true,
  "scripts": {
    "clean": "rimraf out/ dist/",
    "build": "webpack",
    "start": "webpack --env.mode=dev",
    "e2e-test": "mocha --file test/env/dev.ts --recursive --exclude 'test/env/*.ts'",
    "unit-test": "mocha src/**/*.spec.ts",
    "test": "npm run unit-test && npm run build && npm run e2e-test",
    "dist-test": "mocha --file test/env/dist.ts --recursive --exclude 'test/env/*.ts'",
    "predist": "npm run unit-test && webpack --env.mode=dist",
    "dist": "electron-packager dist/tmp --out=dist --overwrite",
    "postdist": "npm run dist-test"
  },
  "repository": "https://github.com/Samarkin/CodeAnalyzer-Electron",
  "keywords": [
    "source code",
    "analysis"
  ],
  "author": "Pavel Samarkin",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.2",
    "electron": "^8.2.1",
    "electron-connect": "^0.6.3",
    "electron-packager": "^14.2.1",
    "electron-reload-webpack-plugin": "^2.0.4",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-react": "^7.19.0",
    "html-webpack-plugin": "^4.2.0",
    "mocha": "^7.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "latest",
    "should": "^13.2.3",
    "spectron": "^10.0.1",
    "style-loader": "^1.1.4",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "@types/mz": "0.0.32",
    "mz": "^2.7.0",
    "typeface-roboto": "0.0.75"
  }
}
